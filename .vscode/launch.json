{
    "version": "0.2.0",
    "compounds": [
        {
            "name": "Python+Java",
            "configurations": [
                "Python",
                "Java"
            ]
        }
    ],
    "configurations": [
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "Java",
            "request": "launch",
            "mainClass": "${file}"
        }
    ]
}